name = "linkup-worker"
main = "build/worker/shim.mjs"
compatibility_date = "2024-05-30"

kv_namespaces = [
  { binding = "LINKUP_SESSIONS", id = "xxx", preview_id = "xxx" },
]

[observability]
enabled = true
head_sampling_rate = 1

[build]
command = "cargo install -q worker-build && worker-build --release"